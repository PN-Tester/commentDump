{
  "manifest_version": 3,

  "name": "CommentDump",
  "description": "Dump all comments from current page and loaded resources into the console",
  "version": "4.0",

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "service_worker": "background.js"
  },

  "permissions": [
    "activeTab",
    "scripting"
  ],

  "host_permissions": [
    "<all_urls>"
  ]
}
